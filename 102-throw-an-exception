public List<Account> checkAccounts(List<Account> accounts)
{
    List<Account> accList = new List<Account>();
    // code here
    try{
        for(Account acc : accounts){
        if (acc.BillingCity == null) throw new AccountException('Invalid BillingCity',e);
        accList.add(acc);
    }
    }
    catch(Exception e){
        return e.message();
    }
    if (accList.equals(accounts)){
        return accounts;
    }
}

//do not remove the following custom-defined exception
public class AccountException extends Exception {}