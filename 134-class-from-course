public Id classFromCourse(String courseName) {
    // code here
    apxio__Course__c crs = [SELECT Id, Name FROM apxio__Course__c WHERE Name = :courseName];

    If(crs.Id != null){
        apxio__Class__c c = new apxio__Class__c(Name = courseName, apxio__Course__c = crs.Id );
        insert c;
        return c.Id;
    }
    else{
        return null;
    }
}