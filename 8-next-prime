public Integer nextPrime(Integer num) {
    //code here
    Boolean isPrime = false;
    do{
        for(Integer i = 2; i < num ; i++){
            if(Math.mod(num,i) == 0){
                num++;
                isPrime = false;
                break;
            }
            else{
                isPrime = true;
            }
        }      
    }
    while(isPrime == false);
    return num;
}