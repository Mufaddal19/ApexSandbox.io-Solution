public Id insertCourse(String name, String details, Integer credits) {
    // code here
    apxio__Course__c course = new apxio__Course__c(Name = name, apxio__Course_Details__c = details, apxio__Credits__c = credits);
    if((Integer)course.apxio__Credits__c <= 4){
        insert course;
        return course.Id;
    }
    else{
        return null;
    }
}