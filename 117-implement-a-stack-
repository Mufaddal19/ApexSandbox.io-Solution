public class Stack {
    
    Integer index = 0;
    Integer temp = 0;
    List<Object> objStack = new List<Object>();
    
    public void push(Object obj) {
        //implement push
        objStack?.add(obj);
        index++;
    }
    
    public Object pop() {
        //implement pop
        return objStack?.remove(--index);
    }
    
    public Integer size() {
        //implement size
        return objStack?.size();
    }
    
    public Object peek(){
        //implement peek
        return objStack.get(index-1);
    }
    
    public Boolean isEmpty() {
        //implement isEmpty
        return objStack.isEmpty();
    }
}