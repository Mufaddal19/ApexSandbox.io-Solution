public Integer maxConsecutiveOnes(Integer[] numbers) {
    //code here
    Integer counter = 0;
    Integer currValue = -1;
    for(Integer i=0;i<numbers.size();i++){
        if(numbers[i] == 1){
            counter++;
        }else if(counter > currValue){
            currValue = counter;
            counter = 0;
        }
    }
    return currValue;
}